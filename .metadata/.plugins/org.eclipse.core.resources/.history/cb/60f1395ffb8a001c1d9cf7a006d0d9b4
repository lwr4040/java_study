package api.lang;

public class Test01 {
	int a = 5;
	
	@Override
	public boolean equals(Object obj) {
		return this.a == ((Test01)obj).a;
	}

	public static void main(String[] args) {
		//Object - 모든 클래스의 조상
		// - 자바에서 만들어진 클래스는 반드시 최고 상위에는 Object클래스를 상속받는다.
		//	(최상위 부모)
		// - 모든 클래스가 기본적으로 가지고 있어야 할 공통적인 기능들을 가지고 있는 클래스
		
		Object a = new Object();
		Object b = new Object();
	}

}
